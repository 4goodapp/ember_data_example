<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
  {{#each contacts}}
    {{view App.ShowContactView contactBinding="this"}}
  {{/each}}
  {{#if showNew}}
    {{view App.NewContactView}}
  {{/if}}
  </tbody>
</table>
<div class="commands">
  {{#view newView}}
    {{link "New Contact"}}
  {{/view}}
  {{#view refreshView}}
    {{link "Refresh Listing"}}
  {{/view}}
</div>
