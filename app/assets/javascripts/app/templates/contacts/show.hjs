<td>{{contact.id}}</td>
<td class="data">
  {{#if view.isEditing}}
    {{view App.EditContactView contactBinding="contact"}}
  {{else}}
    {{contact.fullName}}
  {{/if}}
  <div class="commands">
    {{#unless view.isEditing}}
      <a href="#" {{action showEdit target="view"}}>Edit</a>
      <a href="#" {{action destroyRecord target="view"}}>Remove</a>
    {{/unless}}
  </div>
</td>
