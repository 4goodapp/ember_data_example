<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
  {{#each contact in content}}
    {{#if contact.id}}
      {{view App.ShowContactView contactBinding="contact"}}
    {{/if}}
  {{/each}}
  {{#if view.isNewVisible}}
    <tr>
      <td>*</td>
      <td>
        {{view App.NewContactView}}
      </td>
    </tr>
  {{/if}}
  </tbody>
</table>
<div class="commands">
  <a href="#" {{action "showNew" target="view"}}>New Contact</a>
</div>
